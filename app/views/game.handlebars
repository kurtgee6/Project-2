
  <head>
        <meta charset="UTF-8">
        <!-- Style -->
          <!-- Compiled and minified CSS -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">
        <link rel="stylesheet" type="text/css" href="/css/game.css">

        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
        <!-- Compiled and minified JavaScript -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
        
        <link href="https://fonts.googleapis.com/css?family=Metamorphous" rel="stylesheet">
        <link rel='stylesheet' type='text/css' href='css/game.css'>

        <script src="/socket.io/socket.io.js"></script>
         <script src="/js/phaser.map"></script>
        <script src="/js/phaser.min.js"></script>

        <script src="js/boot.js" type="text/javascript"></script>
        <script src="js/Preload.js" type="text/javascript"></script>
        <script src="js/Town.js" type="text/javascript"></script>
        <script src="js/Battle.js" type="text/javascript"></script>
        <script src="js/Shop.js" type="text/javascript"></script>
        <script src="js/Castle.js" type="text/javascript"></script>
        <script src="js/functionObjects/client.js" type="text/javascript"></script>
        <script src="js/functionObjects/generator.js" type='text/javascript'></script>
        <script src="js/functionObjects/interaction.js" type='text/javascript'></script>
        <script src="js/functionObjects/player.js" type='text/javascript'></script>

  
        
  </head>
  <body>
    <div class="container">
      <div class='row'>
        <div class="col s9">
          <div class='row'>
            <div id='game'>
              <div class='announcement'></div>
              <div class='interaction'></div>
              <div class='battle-info'></div>
              <div class='battle-options'></div>
            </div>
          </div>
        </div>

        <div class="col s3" id='sidebar'>
          <div class="row">
            <div class="col s12">
              <ul class="tabs">
                <li class="tab user-info-tab col s4"><a  class='active' href="#user-stats">Stats</a></li>
                <li class="tab user-info-tab col s4"><a href="#user-equipments">Equipments</a></li>
                <li class="tab user-info-tab col s4"><a href="#user-inventories">Inventory</a></li>
              </ul>
            </div>

            <div id="user-stats" class="tab-item col s12">
              <div id='user-profile'></div>
              <table class='striped'>
                <tbody>
                  <tr>
                    <td><span class='stats-item'>HP</span></td>
                    <td><span id='user-hp'></span></td>
                    <td><span class='stats-item'>MP</span></td>
                    <td><span id='user-mp'></span></td>
                  </tr>
                  <tr>
                    <td><span class='stats-item'>Attack</span></td>
                    <td><span id='user-attack'></span></td>
                    <td><span class='stats-item'>Defense</span></td>
                    <td><span id='user-defense'></span></td>
                  </tr>
                  <tr>
                    <td><span class='stats-item'>Agility</span></td>
                    <td><span id='user-agility'></span></td>
                    <td><span class='stats-item'>Recovery</span></td>
                    <td><span id='user-recovery'>10</span></td>
                  </tr>
                  <tr>
                    <td><span class='stats-item'>Win</span></td>
                    <td><span id='user-win'></span></td>
                    <td><span class='stats-item'>Lose</span></td>
                    <td><span id='user-lose'></span></td>
                  </tr>
                </tbody>
              </table>
              <div id='user-currency'><span id='user-gold'>Gold: </span><span id='user-gold-amount'></span></div>
            </div>
            <div id="user-equipments" class="tab-item col s12">
              <table class='centered striped'>
                <tbody>
                  <tr>
                    <td><span class='equip-item'>Head</span></td>
                    <td><span id='equip-head'></span></td>
                  </tr>
                  <tr>
                    <td><span class='equip-item'>Torso</span></td>
                    <td><span id='equip-torso'></span></td>
                  </tr>
                  <tr>
                    <td><span class='equip-item'>Hand</span></td>
                    <td><span id='equip-hand'></span></td>
                  </tr>
                  <tr>
                    <td><span class='equip-item'>Leg</span></td>
                    <td><span id='equip-leg'></td>
                  </tr>
                  <tr>
                    <td><span class='equip-item'>Feet</span></td>
                    <td><span id='equip-feet'></td>
                  </tr>
                  <tr>
                    <td><span class='equip-item'>Weapon</span></td>
                    <td><span id='equip-weapon'></td>
                  </tr>
                  <tr>
                    <td><span class='equip-item'>Spell</span></td>
                    <td><span id='equip-spell'></td>
                  </tr>
                </tbody>
              </table>
            </div>
            <div id="user-inventories" class="tab-item col s12">
            </div>  
        </div>
        <div class='row'>
          <div id='global-message-box'>
            <div id='global-messages'></div>
            <input type='text' class='message-input' id='global-message-input' placeholder='chat with other players'>
          </div>

          <div id='private-message-box'>
            <div id='private-messages'></div>
            <input type='text' class='message-input' id='private-message-input' placeholder='chat with other players'>
          </div>
        </div>
      </div>
    </div>
    
    <script src="js/functionObjects/user.js" type="text/javascript"></script>
    <script src='js/functionObjects/htmlInteraction.js' type='text/javascript'></script>
    <script src="js/main.js" type="text/javascript"></script>
  </body>
